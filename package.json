{
  "name": "@reasonai/cli",
  "version": "1.0.4",
  "description": "A simple tool for creating AI-based APIs",
  "license": "MIT",
  "type": "module",
  "bin": {
    "reasonai": "./dist/cli.mjs"
  },
  "types": "./dist/cli.d.cts",
  "exports": {
    "require": {
      "types": "./dist/cli.d.cts",
      "default": "./dist/cli.cjs"
    },
    "import": {
      "types": "./dist/cli.d.mts",
      "default": "./dist/cli.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reasonai/cli.git"
  },
  "homepage": "https://reasonai.dev",
  "keyworkds": [
    "ai",
    "node",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/reasonai/cli/issues"
  },
  "author": "Joseph Anane",
  "scripts": {
    "cli": "tsx src/cli.ts",
    "start": "node dist/cli.mjs",
    "build": "pkgroll",
    "type-check": "tsc"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@types/deep-equal": "^1.0.4",
    "deep-equal": "^2.2.3",
    "js-yaml": "^4.1.0",
    "nanoid": "^5.0.7",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "lint-staged": "^15.2.9",
    "pkgroll": "2.4.1",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
  }
}

    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "pkgroll": "2.4.1",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
  }

{
  "name": "@reasonai/cli",
  "version": "1.0.6",
  "description": "A simple tool for creating AI-based APIs",
  "license": "MIT",
  "type": "module",
  "bin": {
    "reasonai": "./dist/cli.mjs"
  },
  "types": "./dist/cli.d.cts",
  "exports": {
    ".": {
      "require": "./dist/cli.cjs",
      "import": "./dist/cli.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reasonai/cli.git"
  },
  "homepage": "https://reasonai.dev",
  "keyworkds": [
    "ai",
    "node",
    "api",
    "functions"
  ],
  "bugs": {
    "url": "https://github.com/reasonai/cli/issues"
  },
  "author": "Joseph Anane",
  "scripts": {
    "cli": "tsx src/cli.ts",
    "start": "node dist/cli.mjs",
    "build": "pkgroll",
    "type-check": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn build"
  },
  "dependencies": {
    "@types/deep-equal": "^1.0.4",
    "deep-equal": "^2.2.3",
    "js-yaml": "^4.1.0",
    "nanoid": "^5.0.7",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "lint-staged": "^15.2.9",
    "pkgroll": "2.4.1",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
  }
}
